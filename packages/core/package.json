{
  "name": "@odavl/core",
  "version": "1.0.1",
  "description": "Shared core utilities for ODAVL Studio",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./manifest": {
      "types": "./dist/manifest/index.d.ts",
      "import": "./dist/manifest/index.js",
      "require": "./dist/manifest/index.cjs"
    },
    "./services/audit-logs": {
      "types": "./dist/services/audit-logs.d.ts",
      "import": "./dist/services/audit-logs.js",
      "require": "./dist/services/audit-logs.cjs"
    },
    "./onboarding-state": {
      "types": "./dist/onboarding-state.d.ts",
      "import": "./dist/onboarding-state.js",
      "require": "./dist/onboarding-state.cjs"
    },
    "./onboarding-api": {
      "types": "./dist/onboarding-api.d.ts",
      "import": "./dist/onboarding-api.js",
      "require": "./dist/onboarding-api.cjs"
    },
    "./services/cli-auth": {
      "types": "./dist/services/cli-auth.d.ts",
      "import": "./dist/services/cli-auth.js",
      "require": "./dist/services/cli-auth.cjs"
    },
    "./services/cli-cloud-upload": {
      "types": "./dist/services/cli-cloud-upload.d.ts",
      "import": "./dist/services/cli-cloud-upload.js",
      "require": "./dist/services/cli-cloud-upload.cjs"
    },
    "./cache/redis-layer": {
      "types": "./dist/cache/redis-layer.d.ts",
      "import": "./dist/cache/redis-layer.js",
      "require": "./dist/cache/redis-layer.cjs"
    },
    "./utils/file-filter": {
      "types": "./dist/utils/file-filter.d.ts",
      "import": "./dist/utils/file-filter.js",
      "require": "./dist/utils/file-filter.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/manifest/index.ts src/services/audit-logs.ts src/onboarding-state.ts src/onboarding-api.ts --format esm,cjs --dts --external yaml --external ajv --external ajv-formats && tsup src/services/cli-auth.ts src/services/cli-cloud-upload.ts src/cache/redis-layer.ts src/utils/file-filter.ts --format esm,cjs --external yaml --external ajv --external ajv-formats --external minimatch",
    "dev": "tsup src/index.ts src/manifest/index.ts --format esm,cjs --dts --watch --external yaml --external ajv --external ajv-formats"
  },
  "keywords": [
    "odavl",
    "utilities"
  ],
  "author": "Mohammad Nawlo",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/s3-request-presigner": "^3.709.0",
    "@prisma/client": "^6.1.0",
    "zod": "^3.23.8",
    "chalk": "^5.3.0",
    "minimatch": "^10.0.1"
  },
  "peerDependencies": {
    "next": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.18"
  }
}
