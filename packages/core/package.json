{
  "name": "@odavl-studio/core",
  "version": "1.0.0",
  "description": "Shared core utilities for ODAVL Studio",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./services/audit-logs": {
      "types": "./dist/services/audit-logs.d.ts",
      "import": "./dist/services/audit-logs.js",
      "require": "./dist/services/audit-logs.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/services/audit-logs.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch"
  },
  "keywords": [
    "odavl",
    "utilities"
  ],
  "author": "Mohammad Nawlo",
  "license": "MIT",
  "dependencies": {
    "@odavl-studio/auth": "workspace:^",
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/s3-request-presigner": "^3.709.0",
    "@prisma/client": "^6.1.0",
    "zod": "^3.23.8",
    "chalk": "^5.3.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.18"
  }
}
