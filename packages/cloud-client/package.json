{
  "name": "@odavl-studio/cloud-client",
  "version": "1.0.0",
  "description": "ODAVL Cloud Client SDK - Connect CLIs to Cloud Platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "keywords": [
    "odavl",
    "cloud",
    "api-client",
    "sdk"
  ],
  "author": "ODAVL Studio",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "nanoid": "^5.1.6",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
