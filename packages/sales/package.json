{
  "name": "@odavl-studio/sales",
  "version": "1.0.0",
  "description": "Sales automation and prospecting tools",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts src/github-prospecting.ts src/stripe-integration.ts --format esm,cjs --dts",
    "prospect": "tsx src/github-prospecting.ts",
    "dev": "tsx watch src/index.ts"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "stripe": "^14.7.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./prospecting": {
      "import": "./dist/github-prospecting.js",
      "require": "./dist/github-prospecting.cjs",
      "types": "./dist/github-prospecting.d.ts"
    },
    "./stripe": {
      "import": "./dist/stripe-integration.js",
      "require": "./dist/stripe-integration.cjs",
      "types": "./dist/stripe-integration.d.ts"
    }
  }
}
