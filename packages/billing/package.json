{
  "name": "@odavl-studio/billing",
  "version": "2.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./insight": {
      "import": "./dist/insight-billing.js",
      "types": "./dist/insight-billing.d.ts"
    },
    "./stripe-mapping": {
      "import": "./dist/stripe-insight-mapping.js",
      "types": "./dist/stripe-insight-mapping.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/insight-billing.ts src/stripe-insight-mapping.ts --format esm --dts",
    "dev": "tsup src/index.ts src/insight-billing.ts src/stripe-insight-mapping.ts --format esm --dts --watch"
  },
  "dependencies": {
    "stripe": "^14.10.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
