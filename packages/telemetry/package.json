{
  "name": "@odavl-studio/telemetry",
  "version": "1.0.0",
  "description": "Enterprise-grade telemetry, logging, metrics, and observability for ODAVL Studio",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --tsconfig tsconfig.json --no-splitting",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["odavl", "telemetry", "logging", "metrics", "observability"],
  "author": "ODAVL Studio",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  }
}
