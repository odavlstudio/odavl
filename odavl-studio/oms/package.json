{
  "name": "@odavl-studio/oms",
  "version": "1.0.0",
  "description": "ODAVL OMS - Operational Management System",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./context": {
      "types": "./dist/oms-context.d.ts",
      "import": "./dist/oms-context.js",
      "require": "./dist/oms-context.cjs"
    },
    "./risk": {
      "types": "./dist/risk/file-risk-index.d.ts",
      "import": "./dist/risk/file-risk-index.js",
      "require": "./dist/risk/file-risk-index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/oms-context.ts src/risk/file-risk-index.ts --format esm,cjs --dts",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "keywords": ["odavl", "oms", "risk"],
  "author": "ODAVL Studio",
  "license": "MIT"
}
