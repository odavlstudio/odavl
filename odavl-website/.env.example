# ODAVL Billing Environment Variables
# Copy to .env.local and fill in actual values

# Stripe Configuration (Required for billing)
STRIPE_SECRET_KEY=sk_test_...
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Product IDs (Set up in Stripe Dashboard)
STRIPE_PRO_PRICE_ID=price_...
STRIPE_ENTERPRISE_PRICE_ID=price_...

# SSO Configuration (Enterprise tier)
SAML_IDP_URL=https://your-idp.com/saml
SAML_ENTITY_ID=odavl-studio
SAML_CERTIFICATE_PATH=/etc/ssl/certs/saml.crt

# Admin Configuration
ADMIN_EMAIL=admin@odavl.studio
SUPPORT_EMAIL=support@odavl.studio