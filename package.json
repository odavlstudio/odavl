{
  "name": "odavl",
  "version": "2.0.0",
  "description": "ODAVL Studio - Unified platform for autonomous code quality with ML-powered error detection, self-healing infrastructure, and pre-deploy testing",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "internal/*"
  ],
  "type": "module",
  "author": "Mohammad Nawlo <mohammad@odavl.studio>",
  "license": "MIT",
  "homepage": "https://github.com/Monawlo812/odavl#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Monawlo812/odavl.git"
  },
  "bugs": {
    "url": "https://github.com/Monawlo812/odavl/issues"
  },
  "keywords": [
    "code-quality",
    "autonomous",
    "ml-powered",
    "error-detection",
    "self-healing",
    "typescript",
    "eslint",
    "vscode-extension",
    "developer-tools",
    "devops",
    "testing",
    "security",
    "performance",
    "pre-deploy",
    "odavl",
    "insight",
    "autopilot",
    "guardian"
  ],
  "engines": {
    "node": ">=18.18",
    "vscode": "^1.85.0"
  },
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "build": "pnpm -r build",
    "lint": "eslint \"apps/**/src/**/*.{ts,tsx}\" \"odavl-studio/**/src/**/*.{ts,tsx}\" \"packages/**/src/**/*.{ts,tsx}\" \"scripts/**/*.ts\" || echo 'Lint completed with warnings in generated files (ignored)'",
    "lint:strict": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:all": "pnpm test && pnpm test:integration && pnpm test:e2e",
    "forensic:all": "pnpm lint && pnpm typecheck && pnpm test:coverage && echo 'Forensic: All checks complete'",
    "dev": "pnpm -r run dev",
    "cli": "node apps/studio-cli/dist/index.js",
    "cli:dev": "tsx apps/studio-cli/src/index.ts",
    "hub:dev": "cd apps/studio-hub && pnpm dev",
    "hub:build": "cd apps/studio-hub && pnpm build",
    "insight:dev": "cd odavl-studio/insight/cloud && pnpm dev",
    "insight:build": "cd odavl-studio/insight/cloud && pnpm build",
    "insight:core": "cd odavl-studio/insight/core && pnpm build",
    "autopilot:engine": "cd odavl-studio/autopilot/engine && pnpm build",
    "guardian:dev": "cd odavl-studio/guardian/app && pnpm dev",
    "guardian:build": "cd odavl-studio/guardian/app && pnpm build",
    "guardian:cli": "cd odavl-studio/guardian/cli && pnpm build",
    "odavl:guardian": "node odavl-studio/guardian/cli/dist/guardian.mjs",
    "guardian": "node odavl-studio/guardian/cli/dist/guardian.mjs",
    "odavl:insight": "tsx odavl-studio/insight/core/scripts/interactive-cli.ts",
    "odavl:autopilot": "tsx odavl-studio/autopilot/engine/scripts/interactive-cli.ts",
    "insight:analyze": "tsx odavl-studio/insight/core/scripts/analyze-stack.ts",
    "insight:root": "tsx odavl-studio/insight/core/scripts/detect-root.ts",
    "insight:fix": "tsx odavl-studio/insight/core/scripts/suggest-fixes.ts",
    "insight:autofix": "tsx odavl-studio/insight/core/scripts/suggest-fixes.ts --auto",
    "insight:learn": "tsx odavl-studio/insight/core/scripts/run-learning.ts",
    "insight:verify": "tsx odavl-studio/guardian/core/src/guardian-cli.ts verify",
    "insight:watch": "tsx odavl-studio/insight/core/scripts/watch-errors.ts",
    "insight:full": "tsx odavl-studio/insight/core/scripts/run-insight.ts",
    "extensions:compile": "pnpm -r --filter './odavl-studio/*/extension' compile",
    "extensions:package": "pnpm -r --filter './odavl-studio/*/extension' package",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "release": "pnpm build && pnpm changeset:publish",
    "ml:collect": "tsx scripts/ml-data-collection.ts",
    "ml:collect-all": "tsx scripts/ml-data-collection.ts --all",
    "ml:prepare": "tsx scripts/ml-prepare-dataset.ts",
    "ml:prepare-all": "tsx scripts/ml-prepare-dataset.ts --all",
    "ml:generate-mock": "tsx scripts/generate-mock-dataset.ts",
    "ml:train": "tsx scripts/ml-train-model.ts",
    "ml:list-models": "tsx scripts/manage-ml-models.ts list",
    "ml:model-info": "tsx scripts/manage-ml-models.ts info",
    "ml:delete-model": "tsx scripts/manage-ml-models.ts delete",
    "monitor:health": "tsx scripts/monitor-odavl-health.ts",
    "monitor:health:verbose": "tsx scripts/monitor-odavl-health.ts --verbose",
    "setup": "tsx scripts/setup-wizard.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "9.39.0",
    "@playwright/test": "^1.49.0",
    "@prisma/client": "^7.0.0",
    "@shadcn/ui": "^0.0.4",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/js-yaml": "^4.0.9",
    "@types/micromatch": "^4.0.10",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-istanbul": "4.0.14",
    "@vitest/coverage-v8": "4.0.14",
    "actionlint": "^2.0.6",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-plugin": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "knip": "^5.67.0",
    "madge": "^8.0.0",
    "markdownlint-cli2": "^0.19.0",
    "micromatch": "^4.0.8",
    "tailwindcss-rtl": "^0.9.0",
    "tsup": "^8.5.1",
    "tsx": "4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "glob": "^11.0.3",
    "js-yaml": "^4.1.1",
    "react-router-dom": "^7.9.5",
    "recharts": "^3.3.0",
    "vite": "7.1.12",
    "yaml": "^2.8.1"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "@eslint/plugin-kit@<0.3.4": ">=0.3.4",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "js-yaml@<4.1.1": ">=4.1.1",
      "next": ">=14.2.25",
      "playwright": ">=1.55.1",
      "hono": ">=4.10.2",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "@types/react": "^19.2.7",
      "@types/react-dom": "^19.2.3",
      "typescript": "^5.9.3",
      "@typescript-eslint/eslint-plugin": "^8.48.0",
      "@typescript-eslint/parser": "^8.48.0",
      "typescript-eslint": "^8.48.0"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "webpack"
      ],
      "allowedVersions": {
        "react": "19",
        "react-dom": "19"
      }
    },
    "overrides": {
      "next": "15.1.6",
      "react": "19.0.0",
      "react-dom": "19.0.0"
    }
  }
}
