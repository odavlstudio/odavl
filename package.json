{
  "name": "odavl",
  "private": true,
  "workspaces": [
    "apps/*",
    "odavl-website"
  ],
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=18.18",
    "vscode": "^1.85.0"
  },
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "build": "pnpm run odavl:run",
    "lint": "eslint . --ext .ts,.js,.tsx,.mjs",
    "typecheck": "tsc --noEmit",
    "odavl:run": "tsx apps/cli/src/index.ts run",
    "odavl:observe": "tsx apps/cli/src/index.ts observe",
    "odavl:decide": "tsx apps/cli/src/index.ts decide",
    "odavl:act": "tsx apps/cli/src/index.ts act",
    "odavl:verify": "tsx apps/cli/src/index.ts verify",
    "ext:compile": "cd apps/vscode-ext && npm run compile",
    "ext:watch": "cd apps/vscode-ext && npm run watch",
    "test": "vitest run",
    "dev": "pnpm -r run dev"
  },
  "devDependencies": {
    "@eslint/js": "9.11.0",
    "@shadcn/ui": "^0.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/recharts": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "2.1.9",
    "actionlint": "^2.0.6",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-plugin": "^4.3.0",
    "eslint-plugin-node": "^11.1.0",
    "tsx": "4.19.1",
    "typescript": "^5.0.0",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "glob": "^11.0.3",
    "js-yaml": "^4.1.0",
    "react-router-dom": "^7.9.4",
    "yaml": "^2.8.1"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "@eslint/plugin-kit@<0.3.4": ">=0.3.4"
    }
  }
    "eslint-config-prettier": "^8.5.0",
  "eslint-plugin-eslint-plugin": "^4.3.0",
  "eslint-plugin-node": "^11.1.0",
  "lucide-react": "^0.545.0",
  "prettier": "^3.3.3",
  "recharts": "^3.2.1",
  "tsx": "4.19.1",
  "typescript": "5.5.4",
  "vitest": "^2.1.1"
},
"dependencies": {
  "glob": "^11.0.3",
  "js-yaml": "^4.1.0",
  "react-router-dom": "^7.9.4",
  "yaml": "^2.8.1"
},
"pnpm": {
  "overrides": {
    "esbuild@<=0.24.2": ">=0.25.0",
    "@eslint/plugin-kit@<0.3.4": ">=0.3.4"
  }
}
}
