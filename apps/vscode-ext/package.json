{
  "name": "odavl-doctor",
  "displayName": "ODAVL Doctor",
  "description": "Live monitoring panel for ODAVL autonomous code quality cycles",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:odavl.doctor"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "odavl.doctor",
        "title": "Open ODAVL Doctor Panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "18.x",
    "typescript": "^4.9.4"
  }
}