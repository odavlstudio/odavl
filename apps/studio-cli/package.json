{
  "name": "@odavl/cli",
  "version": "1.0.0",
  "description": "ODAVL Studio CLI - Production-ready ML-powered code analysis with Insight, Autopilot, and Guardian (Wave 4)",
  "main": "dist/index.js",
  "bin": {
    "odavl": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup && node scripts/add-shebang.cjs",
