    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "odavl",
    "cli",
    "code-quality",
    "error-detection",
    "ml-powered",
    "static-analysis",
    "self-healing",
    "deployment",
    "typescript",
    "python",
    "java",
    "go",
    "rust",
    "automation",
    "testing",
    "cicd"
  ],
  "author": "ODAVL Studio Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/odavlstudio/odavl.git",
    "directory": "apps/studio-cli"
  },
  "bugs": {
    "url": "https://github.com/odavlstudio/odavl/issues"
  },
  "homepage": "https://github.com/odavlstudio/odavl#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@odavl-studio/brain": "workspace:*",
    "@odavl-studio/guardian-core": "workspace:*",
    "@odavl-studio/cloud-client": "workspace:*",
    "@odavl-studio/oms": "workspace:*",
    "@odavl-studio/sdk": "workspace:*",
    "@odavl-studio/auth": "workspace:*",
    "@odavl-studio/telemetry": "workspace:*",
