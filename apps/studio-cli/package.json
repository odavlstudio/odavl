    "@odavl/core": "workspace:*",
    "@odavl/insight-sdk": "workspace:*",
    "@odavl/autopilot-core": "workspace:*",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "ora": "^8.2.0",
    "boxen": "^8.0.1",
    "prompts": "^2.4.2",
    "open": "^10.1.0",
    "date-fns": "^4.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
